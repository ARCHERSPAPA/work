.carousel-swiper {
  width: 100%;
  height: calc((100vw - 36.23rpx) / 2.618);
  border-radius: 14.49rpx;
}
.carousel-swiper-item {
    width: 100%;
    height: 100%;
    border-radius: 14.49rpx;
}
