/**
 * 头部搜索
 */
.mkb-index {
  margin: 0 28.98rpx;
}
.mkb-index .search-bar {
    width: 100%;
    height: 57.97rpx;
    margin-bottom: 28.98rpx;
    position: relative;
}
.mkb-index .search-bar-mask, .mkb-index .search-bar-inp {
      width: 100%;
      height: 100%;
      position: absolute;
      left: 0;
      top: 0;
      z-index: 9;
}
.mkb-index .search-bar-mask {
      z-index: 10;
      opacity: 0;
}
/**
 * 广告banner
