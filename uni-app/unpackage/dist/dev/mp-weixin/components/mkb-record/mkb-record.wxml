<view class="scan"><view class="scan-item"><view class="scan-img"><mkb-img-cut vue-id="1" imgUrl="{{getImgUrl}}" bind:__l="__l"></mkb-img-cut></view></view><view class="scan-item scan-box"><view class="scan-box-item"><text class="scan-box-item-name">{{item.customerHouseAddress}}</text><view class="scan-box-item-state"><mkb-state vue-id="2" state="{{item.state}}" type="{{1}}" bind:__l="__l"></mkb-state></view></view><view class="scan-box-item"><block wx:if="{{item.decorateType}}"><text class="scan-box-item-text">{{item.decorateType}}</text></block><block wx:if="{{item.customerHouseType}}"><text class="scan-box-item-text">{{item.customerHouseType}}</text></block><block wx:if="{{item.customerHouseArea}}"><text class="scan-box-item-text">{{$root.f0+"m²"}}</text></block></view><view class="scan-box-item item-border"><text class="scan-box-item-price">{{"¥ "+$root.f1}}</text><text class="scan-box-item-date">{{$root.f2}}</text></view></view></view>