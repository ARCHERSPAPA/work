<div class="bread" *ngIf="breads && breads.length > 0">
    <nz-breadcrumb>
        <nz-breadcrumb-item *ngFor="let bread of breads;let last = last">
            <a [href]="bread.url" *ngIf="!last">{{bread.name}}</a>
            <span *ngIf="last">{{bread.name}}</span>
        </nz-breadcrumb-item>
    </nz-breadcrumb>
</div>