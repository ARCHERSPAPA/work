@charset "utf-8";
.exam {
  h3 {
    border-bottom: 1px solid rgba(232, 232, 232, 1);
    padding-bottom: 24px;
  }
}
.exam-acrocss {
  margin-top: 10px;
  .lable {
    margin-right: 39px;
    font-size: 14px;
  }
}
.exam-head {
  display: flex;
  justify-content: space-between;
  .exam-li {
    font-size: 14px;
    width: 100%;
    .label {
      text-align: right;
      margin-right: 24px;
      vertical-align: top;
      padding-top: 4px;
    }
    .inp {
      width: 44%;
      display: inline-block;
      vertical-align: text-bottom;
    }
  }
}
.list-group-rev {
  margin-top: 54px;
  font-size: 14px;
  .item-content {
    .item-body {
      .item-block {
        a {
          padding-left: 16px;
        }
      }
      border-bottom: 1px solid rgba(232, 232, 232, 1);
      padding-bottom: 24px;
      .flex7 {
        display: flex;
        justify-content: space-between;
        font-weight: bold;
      }
      .flex2 {
        > span {
          display: inline-block;
          width: 33%;
          margin-top: 8px;
        }
        color: rgba(0, 0, 0, 0.65);
        margin-top: 16px;
      }
    }
  }
}
