@charset "utf-8";
@import "./../../../assets/scss/mixin";
.stencil{
  padding: 24px;
  tr th{
    background:#fafafa;
  }
  tr th, tr td{
    max-width: 80px;
    font-size: 14px;
    @include compatibility(word-break,break-word);
    @include  ellipsis();
  }
  .delete{
    color:#f5222d;
  }
}
tr.selectColor{
  background: #e6f7ff;
}