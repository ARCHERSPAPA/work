                <label class="label"><span class="icon">*</span>客户手机号</label>
                <div class="inp">
                    <input nz-input type="text" class="text" placeholder="请输入客户手机号码" formControlName="customerPhone"
                        name="customerPhone" autocomplete="off" maxlength="16" [(ngModel)]="customerPhone"
                        [ngClass]="{'disabled':controlPhoneByState()}" [readonly]="controlPhoneByState()" />
                    <div [hidden]="clientForm.get('customerPhone').valid || clientForm.get('customerPhone').pristine"
                        class="error">
                        <span [hidden]="!clientForm.hasError('required',['customerPhone'])">必填选项</span>
                        <span [hidden]="!clientForm.hasError('phone',['customerPhone'])">输入手机号码格式不正确</span>
                    </div>
                </div>
            </li> -->
            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>装修地址</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <span nz-col [nzSpan]="10">
                        <input nz-input type="text" class="text hemi" placeholder="楼盘名称" formControlName="address"
                            name="address" autocomplete="off" maxlength="32" [(ngModel)]="address" />
                    </span>
                    <span nz-col [nzSpan]="2" [nzOffset]="2" class="typename">房号</span>
                    <span nz-col [nzSpan]="10" style="float:right">
                        <input nz-input type="text" class="text hemi" placeholder="具体房号" formControlName="roomNumber"
                            name="roomNumber" maxlength="10" autocomplete="off" [(ngModel)]="roomNumber" />
                    </span>
                    <nz-form-explain *ngIf="clientForm.get('address').dirty && clientForm.get('address').errors">
                        <ng-container *ngIf="clientForm.get('address').hasError('required')">
                            必填地址信息
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('address').hasError('account')">
                            输入地址格式不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('address').hasError('minlength')">
                            &nbsp;&nbsp;至少输入1位字符
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('address').hasError('maxlength')">
                            &nbsp;&nbsp;至多输入30位字符
                        </ng-container>
                    </nz-form-explain>
                    <nz-form-explain *ngIf="clientForm.get('roomNumber').dirty && clientForm.get('roomNumber').errors">
                        <ng-container *ngIf="clientForm.get('roomNumber').hasError('minlength')">
                            &nbsp;&nbsp;至少输入1位字符
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('roomNumber').hasError('maxlength')">
                            &nbsp;&nbsp;至多输入10位字符
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
