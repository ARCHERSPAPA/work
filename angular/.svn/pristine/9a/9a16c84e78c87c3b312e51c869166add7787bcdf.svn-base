                        <ng-container *ngIf="clientForm.get('remarks').hasError('maxlength')">
                            至多输入120位字符
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <li>
                <div class="btns">
                    <button nz-button nzType="default" (click)="exist($event)">关闭</button>
                    <button nz-button nzType="primary" [disabled]="!clientForm.valid" (click)="submit($event)">修改
                    </button>
                </div>
            </li>
        </form>
    </ul>

