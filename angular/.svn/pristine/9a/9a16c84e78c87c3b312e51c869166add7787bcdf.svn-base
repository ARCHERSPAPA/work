                        </nz-option>
                    </nz-select>
                    <input nz-input type="hidden" formControlName="sourceInfo" name="sourceInfo"
                        [(ngModel)]="sourceInfo" />
                    <nz-form-explain *ngIf="clientForm.get('sourceId').dirty && clientForm.get('sourceId').errors">
                        <ng-container *ngIf="clientForm.get('sourceId').hasError('required')">
                            必选客户来源
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="detail-add-title layout" style="margin-top: 24px;">
