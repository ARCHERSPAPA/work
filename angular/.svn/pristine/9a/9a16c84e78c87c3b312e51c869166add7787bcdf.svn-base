<div class="detail-add">
    <ul *ngIf="type === 9">
        <form [formGroup]="clientForm">
            <nz-form-item class="detail-add-title layout">
                <nz-form-label [nzSpan]="5">部门及负责人</nz-form-label>
            </nz-form-item>
            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>所属部门</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <div nz-col [nzSpan]="20" class="inp-text" (click)="selectDepart()"></div>
                    <input nz-input type="text" class="text" placeholder="请选择所属部门" formControlName="departmentName"
                        name="departmentName" [(ngModel)]="departmentName" readonly />

                    <input nz-input type="hidden" formControlName="departmentId" name="departmentId" autocomplete="off"
                        [(ngModel)]="departmentId" />
                    <nz-form-explain
                        *ngIf="clientForm.get('departmentId').dirty && clientForm.get('departmentId').errors">
                        <ng-container *ngIf="clientForm.get('departmentId').hasError('required')">
                            必选所属部门
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5">负责人</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <div nz-col [nzSpan]="20" class="inp-text" (click)="selectRelevant(1)"></div>
                    <input nz-input type="hidden" formControlName="personLiableId" name="personLiableId"
                        [(ngModel)]="personLiableId" />
                    <input nz-input type="text" class="text" placeholder="请选择负责人" formControlName="personLiableName"
                        name="personLiableName" [(ngModel)]="personLiableName" readonly="readonly" />
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>客户来源</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <nz-select [(ngModel)]="sourceId" (ngModelChange)="selectSource()" formControlName="sourceId"
                        name="sourceId" nzAllowClear nzPlaceHolder="请选择客户来源渠道">
                        <nz-option *ngFor="let source of sourceList" [nzValue]="source.id" [nzLabel]="source.name">
                        </nz-option>
                    </nz-select>
                    <input nz-input type="hidden" formControlName="sourceInfo" name="sourceInfo"
                        [(ngModel)]="sourceInfo" />
                    <nz-form-explain *ngIf="clientForm.get('sourceId').dirty && clientForm.get('sourceId').errors">
                        <ng-container *ngIf="clientForm.get('sourceId').hasError('required')">
                            必选客户来源
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="detail-add-title layout" style="margin-top: 24px;">
                <nz-form-label [nzSpan]="5">客户资料</nz-form-label>
            </nz-form-item>

            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>客户姓名</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <input nz-input type="text" class="text" placeholder="请输入客户姓名" formControlName="customerName"
                        name="customerName" autocomplete="off" maxlength="50" [(ngModel)]="customerName" />
                    <nz-form-explain
                        *ngIf="clientForm.get('customerName').dirty && clientForm.get('customerName').errors">
                        <ng-container *ngIf="clientForm.get('customerName').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerName').hasError('text')">
                            &nbsp;&nbsp;只能输入英文或者中文
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerName').hasError('minlength')">
                            &nbsp;&nbsp;至少输入2位字符
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerName').hasError('maxlength')">
                            &nbsp;&nbsp;至多输入50位字符
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <!-- <li>
                <label class="label"><span class="icon">*</span>客户姓名</label>
                <div class="inp">
                    <input nz-input type="text" class="text" placeholder="请输入客户姓名" formControlName="customerName"
                        name="customerName" autocomplete="off" maxlength="50" [(ngModel)]="customerName" />
                    <div [hidden]="clientForm.get('customerName').valid || clientForm.get('customerName').pristine"
                        class="error">
                        <span [hidden]="!clientForm.hasError('required',['customerName'])">必填选项</span>
                        <span [hidden]="!clientForm.hasError('text',['customerName'])">&nbsp;&nbsp;只能输入英文或者中文</span>
                        <span [hidden]="!clientForm.hasError('minlength',['customerName'])">&nbsp;&nbsp;至少输入2位字符</span>
                        <span [hidden]="!clientForm.hasError('maxlength',['customerName'])">&nbsp;&nbsp;至多输入50位字符</span>
                    </div>
                </div>
            </li> -->
            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>客户手机号</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <input nz-input type="text" class="text" placeholder="请输入客户手机号码" formControlName="customerPhone"
                        name="customerPhone" autocomplete="off" maxlength="16" [(ngModel)]="customerPhone"
                        [ngClass]="{'disabled':controlPhoneByState()}" [readonly]="controlPhoneByState()" />
                    <nz-form-explain
                        *ngIf="clientForm.get('customerPhone').dirty && clientForm.get('customerPhone').errors">
                        <ng-container *ngIf="clientForm.get('customerPhone').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerPhone').hasError('phone')">
                            输入手机号码格式不正确
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
