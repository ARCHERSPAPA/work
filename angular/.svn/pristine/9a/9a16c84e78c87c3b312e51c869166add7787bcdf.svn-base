    <ul *ngIf="type === 12">
        <form [formGroup]="clientForm">
            <nz-form-item class="layout" *ngIf="newcase">
                <nz-form-label nzRequired [nzSpan]="5">楼盘名称</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <input nz-input type="text" [ngStyle]="{'cursor':!newcase ? 'not-allowed' : 'auto' }" class="text "
                        placeholder="请输入楼盘名称" formControlName="customerHouseAddress"
                        (blur)="customerHouseAddress=trims(customerHouseAddress)" maxlength="16" minlength="1"
                        name="customerHouseAddress" autocomplete="off" [(ngModel)]="customerHouseAddress"
                        [readonly]="!newcase" />
                    <nz-form-explain
                        *ngIf="clientForm.get('customerHouseAddress').dirty && clientForm.get('customerHouseAddress').errors">
                        <ng-container *ngIf="clientForm.get('customerHouseAddress').hasError('required')">
                            必填选项
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout" *ngIf="!newcase">
                <nz-form-label [nzSpan]="5">客户姓名</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <input nz-input type="text"
                        [ngStyle]="{'cursor':!newcase ? 'not-allowed' : 'auto','color':newcase?'rgba(0,0,0,0.25)':'rgba(0,0,0,0.25)' }"
                        class="text " placeholder="请输入客户姓名" formControlName="customerName" name="customerName"
                        autocomplete="off" maxlength="50" [(ngModel)]="customerName" [readonly]="!newcase" />
                    <nz-form-explain
                        *ngIf="clientForm.get('customerName').dirty && clientForm.get('customerName').errors">
                        <ng-container *ngIf="clientForm.get('customerName').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerName').hasError('text')">
                            &nbsp;&nbsp;只能输入英文或者中文
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerName').hasError('minlength')">
                            &nbsp;&nbsp;至少输入2位字符
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerName').hasError('maxlength')">
                            &nbsp;&nbsp;至多输入50位字符
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout" *ngIf="!newcase">
                <nz-form-label [nzSpan]="5">客户手机号</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <input nz-input type="text" class="text "
                        [ngStyle]="{'cursor':!newcase ? 'not-allowed' : 'auto','color':newcase?'rgba(0,0,0,0.25)':'rgba(0,0,0,0.25)' }"
                        placeholder="请输入客户手机号码" formControlName="customerPhone" name="customerPhone" autocomplete="off"
                        maxlength="16" [(ngModel)]="customerPhone" [readonly]="!newcase" />
                    <nz-form-explain
                        *ngIf="clientForm.get('customerPhone').dirty && clientForm.get('customerPhone').errors">
                        <ng-container *ngIf="clientForm.get('customerPhone').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerPhone').hasError('phone')">
                            输入手机号码格式不正确
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout" *ngIf="newcase">
                <nz-form-label [nzSpan]="5" nzRequired>设计师手机号</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <input nz-input type="text" class="text " placeholder="请输入设计师手机号码" formControlName="phone"
                        name="phone" autocomplete="off" maxlength="16" [(ngModel)]="phone" />
                    <nz-form-explain *ngIf="clientForm.get('phone').dirty && clientForm.get('phone').errors">
                        <ng-container *ngIf="clientForm.get('phone').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('phone').hasError('phone')">
                            输入手机号码格式不正确
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout" *ngIf="newcase">
                <nz-form-label [nzSpan]="5" nzRequired>合同金额</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <nz-input-number placeholder="请输入合同金额" style="width:100%" formControlName="finalPrice"
                        name="finalPrice" autocomplete="off" [(ngModel)]="finalPrice" [nzMin]="1" [nzStep]=".1"
                        [nzDisabled]="!newcase"></nz-input-number>
                    <nz-form-explain *ngIf="clientForm.get('finalPrice').dirty && clientForm.get('finalPrice').errors">
                        <ng-container *ngIf="clientForm.get('finalPrice').hasError('required')">
                            必填选项
                        </ng-container>
                        <!-- <ng-container *ngIf="clientForm.get('contractPrice').hasError('phone')">
                                输入手机号码格式不正确
                            </ng-container> -->
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout" *ngIf="!newcase" nzRequired>
                <nz-form-label [nzSpan]="5">装修地址</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <span nz-col [nzSpan]="10">
                        <input nz-input type="text" class="text hemi "
                            [ngStyle]="{'cursor':!newcase ? 'not-allowed' : 'auto','color':newcase?'rgba(0,0,0,0.25)':'rgba(0,0,0,0.25)' }"
                            placeholder="楼盘名称" formControlName="address" name="address" autocomplete="off"
                            maxlength="32" [(ngModel)]="address" [readonly]="!newcase" />
                    </span>
                    <span nz-col [nzSpan]="2" [nzOffset]="2" class="typename ">房号</span>
                    <span nz-col [nzSpan]="10" style="float:right">
                        <input nz-input type="text" class="text hemi "
                            [ngStyle]="{'cursor':!newcase ? 'not-allowed' : 'auto','color':newcase?'rgba(0,0,0,0.25)':'rgba(0,0,0,0.25)' }"
                            placeholder="具体房号" formControlName="roomNumber" name="roomNumber" maxlength="10"
                            autocomplete="off" [(ngModel)]="roomNumber" [readonly]="!newcase" />
                    </span>
                    <nz-form-explain *ngIf="clientForm.get('address').dirty && clientForm.get('address').errors">
                        <ng-container *ngIf="clientForm.get('address').hasError('required')">
                            必填地址信息
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('address').hasError('account')">
                            输入地址格式不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('address').hasError('minlength')">
                            &nbsp;&nbsp;至少输入1位字符
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('address').hasError('maxlength')">
                            &nbsp;&nbsp;至多输入30位字符
                        </ng-container>
                    </nz-form-explain>
                    <nz-form-explain *ngIf="clientForm.get('roomNumber').dirty && clientForm.get('roomNumber').errors">
                        <ng-container *ngIf="clientForm.get('roomNumber').hasError('minlength')">
                            &nbsp;&nbsp;至少输入1位字符
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('roomNumber').hasError('maxlength')">
                            &nbsp;&nbsp;至多输入10位字符
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout" *ngIf="newcase">
                <nz-form-label [nzSpan]="5">地址定位</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <div class="inp-text" (click)="openModal('address',customerGpsAddress)"></div>
                    <input nz-input type="text" class="text" placeholder="请选择地址" readonly="readonly"
                        formControlName="customerGpsAddress" name="customerGpsAddress"
                        [(ngModel)]="customerGpsAddress" />
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout" *ngIf="!newcase">
                <nz-form-label [nzSpan]="5" nzRequired>地址定位</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <div class="inp-text" (click)="openModal('address',customerGpsAddress)"></div>
                    <input nz-input type="text" class="text" placeholder="请选择地址" readonly="readonly"
                        formControlName="customerGpsAddress" name="customerGpsAddress"
                        [(ngModel)]="customerGpsAddress" />
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout" *ngIf="newcase">
                <nz-form-label [nzSpan]="5" nzRequired>房屋户型</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <span nz-col [nzSpan]="12">
                        <nz-input-number placeholder="请输入房间数" style="width:100%" formControlName="roomType"
                            name="roomType" autocomplete="off" [(ngModel)]="roomType"
                            [ngClass]="{'disabled':controlHouseTypeByState()}" [nzMin]="0" [nzMax]="100" [nzStep]="1"
                            [nzDisabled]="!newcase"></nz-input-number>
                        <!-- <input nz-input type="text" class="text hemi" placeholder="请输入房间数"
                        formControlName="roomType" name="roomType" maxlength="3" autocomplete="off"
                        [(ngModel)]="roomType" [ngClass]="{'disabled':controlHouseTypeByState()}"
                        [readonly]="controlHouseTypeByState()" /> -->
                    </span><span class="typename position-name" style="right: 263px;">室</span>
                    <span nz-col [nzSpan]="12">
                        <nz-input-number placeholder="请输入卫生间数" style="width:100%" formControlName="toiletType"
                            name="toiletType" autocomplete="off" [(ngModel)]="toiletType"
                            [ngClass]="{'disabled':controlHouseTypeByState()}" [nzMin]="0" [nzMax]="100" [nzStep]="1"
                            [nzDisabled]="!newcase"></nz-input-number>
                        <!-- <input nz-input type="text" class="text  hemi" placeholder="请输入卫生间数"
                        formControlName="toiletType" name="toiletType" maxlength="3" autocomplete="off"
                        [(ngModel)]="toiletType" [ngClass]="{'disabled':controlHouseTypeByState()}"
                        [readonly]="controlHouseTypeByState()" /> -->
                    </span><span class="typename position-name" style="right: 24px;">卫</span>
                    <nz-form-explain *ngIf="clientForm.get('roomType').dirty && clientForm.get('roomType').errors">
                        <ng-container *ngIf="clientForm.get('roomType').hasError('numberMin')">
                            输入房间数量不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('roomType').hasError('min')">
                            输入房间数量至少为0
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('roomType').hasError('max')">
                            输入卫生间数量至多为100
                        </ng-container>
                    </nz-form-explain>
                    <nz-form-explain *ngIf="clientForm.get('toiletType').dirty && clientForm.get('toiletType').errors">
                        <ng-container *ngIf="clientForm.get('toiletType').hasError('numberMin')">
                            输入房间数量不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('toiletType').hasError('min')">
                            &nbsp;&nbsp;输入房间数量至少为0
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('toiletType').hasError('max')">
                            &nbsp;&nbsp;输入卫生间数量至多为100
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout" *ngIf="!newcase">
                <nz-form-label [nzSpan]="5">房屋户型</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <span nz-col [nzSpan]="12">
                        <nz-input-number placeholder="请输入房间数" style="width:100%" formControlName="roomType"
                            name="roomType" autocomplete="off" [(ngModel)]="roomType"
                            [ngClass]="{'disabled':controlHouseTypeByState()}" [nzMin]="0" [nzMax]="100" [nzStep]="1"
                            [nzDisabled]="!newcase"></nz-input-number>
                        <!-- <input nz-input type="text" class="text hemi" placeholder="请输入房间数"
                        formControlName="roomType" name="roomType" maxlength="3" autocomplete="off"
                        [(ngModel)]="roomType" [ngClass]="{'disabled':controlHouseTypeByState()}"
                        [readonly]="controlHouseTypeByState()" /> -->
                    </span><span class="typename position-name" style="right: 263px;">室</span>
                    <span nz-col [nzSpan]="12">
                        <nz-input-number placeholder="请输入卫生间数" style="width:100%" formControlName="toiletType"
                            name="toiletType" autocomplete="off" [(ngModel)]="toiletType"
                            [ngClass]="{'disabled':controlHouseTypeByState()}" [nzMin]="0" [nzMax]="100" [nzStep]="1"
                            [nzDisabled]="!newcase"></nz-input-number>
                        <!-- <input nz-input type="text" class="text  hemi" placeholder="请输入卫生间数"
                        formControlName="toiletType" name="toiletType" maxlength="3" autocomplete="off"
                        [(ngModel)]="toiletType" [ngClass]="{'disabled':controlHouseTypeByState()}"
                        [readonly]="controlHouseTypeByState()" /> -->
                    </span><span class="typename position-name" style="right: 24px;">卫</span>
                    <nz-form-explain *ngIf="clientForm.get('roomType').dirty && clientForm.get('roomType').errors">
                        <ng-container *ngIf="clientForm.get('roomType').hasError('numberMin')">
                            输入房间数量不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('roomType').hasError('min')">
                            输入房间数量至少为0
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('roomType').hasError('max')">
                            输入卫生间数量至多为100
                        </ng-container>
                    </nz-form-explain>
                    <nz-form-explain *ngIf="clientForm.get('toiletType').dirty && clientForm.get('toiletType').errors">
                        <ng-container *ngIf="clientForm.get('toiletType').hasError('numberMin')">
                            输入房间数量不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('toiletType').hasError('min')">
                            &nbsp;&nbsp;输入房间数量至少为0
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('toiletType').hasError('max')">
                            &nbsp;&nbsp;输入卫生间数量至多为100
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout" *ngIf="!newcase">
                <nz-form-label [nzSpan]="5">房屋面积</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <nz-input-number placeholder="请输入客户的房屋面积" style="width:100%" formControlName="houseArea"
                        name="houseArea" autocomplete="off" class="disabled" [(ngModel)]="houseArea" [nzMin]="1"
                        [nzMax]="99999" [nzStep]="1" [nzDisabled]="!newcase"></nz-input-number>
                    <span class="position-name" style="right: 24px;">
                        m<sup>2</sup>
                    </span>
                    <!-- <input nz-input type="text" class="text" placeholder="请输入客户的房屋面积" formControlName="houseArea"
                    name="houseArea" autocomplete="off" maxlength="6" [(ngModel)]="houseArea"
                    [ngClass]="{'disabled':controlHouseTypeByState()}" [readonly]="controlHouseTypeByState()" /> -->
                    <nz-form-explain *ngIf="clientForm.get('houseArea').dirty && clientForm.get('houseArea').errors">
                        <ng-container *ngIf="clientForm.get('houseArea').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('houseArea').hasError('decimal')">
                            &nbsp;&nbsp;输入房屋面积不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('houseArea').hasError('min')">
                            &nbsp;&nbsp;至少输入比1大
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('houseArea').hasError('max')">
                            &nbsp;&nbsp;至多输入为99999
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout" *ngIf="newcase">
                <nz-form-label [nzSpan]="5" nzRequired>房屋面积</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <nz-input-number placeholder="请输入客户的房屋面积" style="width:100%" formControlName="customerHouseArea"
                        name="customerHouseArea" autocomplete="off" class="disabled" [(ngModel)]="customerHouseArea"
                        [nzMin]="1" [nzMax]="99999" [nzStep]="1" [nzDisabled]="!newcase"></nz-input-number>
                    <span class="position-name" style="right: 24px;">
                        m<sup>2</sup>
                    </span>
                    <!-- <input nz-input type="text" class="text" placeholder="请输入客户的房屋面积" formControlName="houseArea"
                    name="houseArea" autocomplete="off" maxlength="6" [(ngModel)]="houseArea"
                    [ngClass]="{'disabled':controlHouseTypeByState()}" [readonly]="controlHouseTypeByState()" /> -->
                    <nz-form-explain
                        *ngIf="clientForm.get('customerHouseArea').dirty && clientForm.get('customerHouseArea').errors">
                        <ng-container *ngIf="clientForm.get('customerHouseArea').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerHouseArea').hasError('decimal')">
                            &nbsp;&nbsp;输入房屋面积不正确
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerHouseArea').hasError('min')">
                            &nbsp;&nbsp;至少输入比1大
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerHouseArea').hasError('max')">
                            &nbsp;&nbsp;至多输入为99999
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <!-- <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" style='vertical-align: top;line-height: 19px;'>备注</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <textarea nz-input class="text textarea" placeholder="请输入备注信息" formControlName="remarks"
                    name="remarks" autocomplete="off" [(ngModel)]="remarks" maxlength="132"></textarea>
                <span class="count" *ngIf="remarks && remarks.length > 0"><span>{{remarks.length}}</span>/120</span>
                <nz-form-explain *ngIf="clientForm.get('remarks').dirty && clientForm.get('remarks').errors">
                    <ng-container *ngIf="clientForm.get('remarks').hasError('maxlength')">
                        至多输入120位字符
                    </ng-container>
                </nz-form-explain>
                </nz-form-control>
            </nz-form-item> -->
            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>装修类型</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <nz-select nzShowSearch nzPlaceHolder="选择装修类型" [(ngModel)]="decorateType"
                        formControlName="decorateType">
                        <nz-option *ngFor="let type of selectedType" [nzLabel]="type.decorateStyle"
                            [nzValue]="type.decorateStyle"></nz-option>
                    </nz-select>
                    <nz-form-explain
                        *ngIf="clientForm.get('decorateType').dirty && clientForm.get('decorateType').errors">
                        <ng-container *ngIf="clientForm.get('decorateType').hasError('required')">
                            必填选项
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <nz-form-item class="layout">
                <nz-form-label [nzSpan]="5" nzRequired>装修风格</nz-form-label>
                <nz-form-control [nzSpan]="19" style="text-align: left;">
                    <nz-select nzShowSearch nzPlaceHolder="选择装修风格" [(ngModel)]="style" formControlName="style">
                        <nz-option *ngFor="let style of decorateStyle" [nzLabel]="style.decorateStyle"
                            [nzValue]="style.decorateStyle"></nz-option>
                    </nz-select>
                    <nz-form-explain *ngIf="clientForm.get('style').dirty && clientForm.get('style').errors">
                        <ng-container *ngIf="clientForm.get('style').hasError('required')">
                            必填选项
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>
            <li>
                <div class="btns">
                    <button nz-button nzType="default" (click)="exist($event)">关闭</button>
                    <button nz-button nzType="primary" [disabled]="!clientForm.valid" *ngIf="!newcase"
                        (click)="submit($event)">修改
                    </button>
                    <button nz-button nzType="primary" [disabled]="!clientForm.valid" *ngIf="newcase"
                        (click)="submit($event)">提交
                    </button>
                </div>
            </li>
        </form>
    </ul>
</div>