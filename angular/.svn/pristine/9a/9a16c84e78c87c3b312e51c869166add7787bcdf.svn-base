                        [ngClass]="{'disabled':controlPhoneByState()}" [readonly]="controlPhoneByState()" />
                    <nz-form-explain
                        *ngIf="clientForm.get('customerPhone').dirty && clientForm.get('customerPhone').errors">
                        <ng-container *ngIf="clientForm.get('customerPhone').hasError('required')">
                            必填选项
                        </ng-container>
                        <ng-container *ngIf="clientForm.get('customerPhone').hasError('phone')">
                            输入手机号码格式不正确
                        </ng-container>
                    </nz-form-explain>
                </nz-form-control>
            </nz-form-item>

            <nz-form-item class="layout">
