@charset "utf-8";
@import "../../../../assets/scss/mixin";
.ant-table-tbody>tr>td span,a{
  max-width: 120px;
  display:inline-block;
  vertical-align: middle;
  text-decoration: none;
  @include ellipsis;
  &.master-img{
    width: 50px;
    height: 50px;
    border-radius: 50%;
    > img{
      width: 100%;
      height: 100%;
    }
  }
}

.dead{
  color:rgba(0,0,0,0.4)!important;
}
.textarea{
  padding:5px 0 14px 0;
  position: relative;
  .count{
    position: absolute;
    right: 5px;
    bottom: -20px;
    font-size: 14px;
    > span{
      color: #F92650;
    }
  }
}
