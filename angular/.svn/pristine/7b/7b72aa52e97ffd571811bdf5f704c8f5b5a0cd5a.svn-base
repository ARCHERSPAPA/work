@charset "utf-8";
@import "./../../../../assets/scss/mixin";

.detail-contract {
  margin: 24px 0;
  .btn-fix{
    text-align: right;
    margin-top: 16px;
//     position: fixed;
//     bottom: 50%;
//     left: 50%;
    button:last-child{
margin-left: 24px;
    }
  }
  .contract-head {
    font-size: 0;
    margin-bottom: 20px;
    overflow: hidden;
    &:after {
      content: "";
      clear: both;
    }
    .title {
      display: inline-block;
      font-size: 16px;
      font-family: "PingFang SC";
      font-weight: bold;
      line-height: 24px;
      color: rgba(0, 0, 0, 0.85);
      margin: 0;
    }
    .fix {
      float: right;
      > a {
        display: inline-block;
        font-size: 12px;
        line-height: 24px;
        margin-left: 10px;
      }
    }
    .contract-icon{
      font-size: 14px;
      color: #000;
    }
  }
  .textarea{
    position: relative;
    .count{
      position:absolute;
      right: 5px;
      bottom: -20px;
      > span{
        color: #f92650;
      }
    }
  }
}
tr th,tr td{
  max-width: 100px;
  font-size: 14px;
  @include ellipsis();
}
.padCut{
  padding: 0 !important;
}