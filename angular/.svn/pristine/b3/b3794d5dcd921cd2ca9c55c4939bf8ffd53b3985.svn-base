@import "./../../../../../assets/scss/mixin";
.cost-base{
    &-list{
        tr th,tr td{
            font-size: 14px;
            max-width:100px;
            vertical-align: middle;
            a{
                padding-right: 4px;
            }
            @include ellipsis();
           }
    }
    .serch{
     display: flex;
     justify-content: space-between;
     .inputCompact{
         width: 528px;
         display: inline-flex;
         text-align: right;
     }
        input{
            width: 200px;
            margin-right: 8px;
        }
    }
}