@import "./../../../../../assets/scss/mixin";
.supplier-materialList {
    &-head {
        margin-top: 24px;
        display: flex;
        justify-content: space-between;
        div{
            input{
                width: 240px;
                margin-right: 8px;
            }
        }
    }
    &-list{
        margin-top: 16px;
        .list-remark{
            width: 118px;
            @include compatibility(word-break, break-word);
        }
        tr th,tr td{
            font-size: 14px;
            max-width:100px;
            vertical-align: middle;
            a{
                padding-right: 4px;
            }
            @include ellipsis();
           }
    }
}
