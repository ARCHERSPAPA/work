@charset "utf-8";
@import "./../../../assets/scss/mixin";
.select-bar{
  display: flex;
  justify-content: flex-end;
  align-items: top;
  .select-bar{
    ::ng-deep .ant-select-selection--multiple{
      max-height: 32px;
      .ant-select-selection__rendered{
        max-height: 32px;
      }
    }
    ::ng-deep ul{
      white-space:nowrap;
      overflow: auto;
      li{
        display:inline-block;
        float: none
      }
    }

  }
  .info{
    width: 240px;
    margin-right: 8px;
    display: inline-block;
  }
}