<div class="ant-modal-content">
    <button type="button" class="ant-modal-close" aria-label="Close" (click)="activeModal.dismiss()">
        <span class="ant-modal-close-x"><i nz-icon type="close"></i></span>
    </button>
    <div class="ant-modal-header">
        <div class="ant-modal-title">
            <p>{{title}}</p>
            <p *ngIf="image && image.createTime" style="font-size: 14px;color: rgba(0,0,0,0.45)">提交时间 {{image.createTime|date:'yy-MM-dd HH:mm'}}</p>
        </div>
    </div>
    <!--<div class="ant-modal-body clearfix">-->
        <!--<a href="javascript:void(0)" (click)="onReset($event)">复位</a>-->
        <!--<div class="image-large-wrapper" *ngIf="src">-->
            <!--<div class="image-large" (mousemove)="onMove($event)" id="moveLarge"-->
                 <!--(mousedown)="onDown($event)" (mouseup)="onUp($event)" (mouseleave)="onLeave($event)">-->
                <!--<img [src]="src" (mousewheel)="onMouseWheel($event)" id="imageLarge"/>-->
            <!--</div>-->
        <!--</div>-->
    <!--</div>-->


    <div class="ant-modal-body clearfix">
        <!--<a href="javascript:void(0)" (click)="onReset($event)">复位</a>-->
        <div class="image-large-wrapper">
            <div class="image-large" id="moveLarge"  (mousemove)="onMove($event)" id="moveLarge"
                (mousedown)="onDown($event)" (mouseup)="onUp($event)" (mouseleave)="onLeave($event)">
               <img [src]="image.url" (mousewheel)="onMouseWheel($event)" (dblclick)="onZoom($event)" id="imageLarge"/>
           </div>
            <div class="image-arrow arrow-left" (click)="onPrev($event)" title="上一张"><i nz-icon type="left" theme="outline"></i></div>
            <div class="image-arrow arrow-right" (click)="onNext($event)" title="下一张"><i nz-icon type="right" theme="outline"></i></div>
       </div>
   </div>


    <div class="ant-modal-footer">
        <button nz-button nzType="primary" (click)="downLoad($event,image)">下载单张</button>
        <button nz-button nzType="primary" *ngIf="isAudit" (click)="setAudit($event,image)">{{image.examineState?'至为未审':'至为审核'}}</button>
    </div>


</div>
