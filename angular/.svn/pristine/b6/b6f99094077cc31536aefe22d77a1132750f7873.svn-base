.cost-disclose {
    font-size: 14px;
    &-title {
        display: flex;
        justify-content: space-between;
    }
    &-title-job{
        color:rgba(0,0,0,0.45);
    } 
    &-title-time{
        color:rgba(0,0,0,0.25);
    } 
    & p{
        color:rgba(0,0,0,0.65);
    }
    &-imgs {
        margin-top: 8px;
        div {
            display: inline-block;
            margin-right: 16px;
            width: 96px;
            background-size: 96px;
            height: 96px;
        }
    }
}
.cost-disclose:nth-child(n+2){
    margin-top: 40px;
}
