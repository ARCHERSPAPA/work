.editable-cell-icon{
    float: right;
    cursor: pointer;
    opacity: 0;
    color: blue;
}
.editable-check{
    float: right;
    cursor: pointer;
    line-height: 24px;
    color: blue;
}
td:hover{
    .editable-cell-icon{
        opacity:1;
    }
}
td {
    input{
        width: 85%;
    }
    a{
        color: #1890ff;
        margin-right: 10px;
    }
}
