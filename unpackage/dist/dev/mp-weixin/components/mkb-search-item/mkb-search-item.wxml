<view class="mkb-tj-box"><view class="mkb-tj-box-item-image"><image class="mkb-tj-box-item-img" src="{{getImgUrl}}" lazy-load="{{true}}"></image><block wx:if="{{item.styleImg}}"><view class="mkb-tj-box-item-flag"><view class="iconfont _i" style="font-size:28.98rpx;"></view></view></block><block wx:if="{{item.vrLiveImg}}"><view class="mkb-tj-box-item-vrimage"><view class="iconfont _i" style="font-size:28.98rpx;"></view></view></block><block wx:if="{{item.vrResultImg}}"><view class="mkb-tj-box-item-vrimg"><view class="iconfont _i" style="font-size:28.98rpx;"></view></view></block></view><view class="mkb-tj-box-item-block"><text class="mkb-tj-box-item-block-name">{{item.customerHouseAddress}}</text><view class="mkb-tj-box-item-block-price"><text>{{"¥"+$root.f0}}</text></view></view><view class="mkb-tj-box-item-area"><text class="mkb-tj-box-item-area-layout"><block wx:if="{{item.customerHouseType}}"><text class="item-layout-text">{{item.customerHouseType}}</text></block><block wx:if="{{item.customerHouseArea}}"><text class="item-layout-text">{{$root.f1+"m²"}}</text></block></text><block wx:if="{{item.likeNum}}"><view class="mkb-tj-box-item-area-zan"><view class="iconfont _i" style="font-size:28.98rpx;color:rgba(0,0,0,0.4);display:inline-block;"></view><text>{{item.likeNum}}</text></view></block></view><view class="mkb-tj-box-item-type"><block wx:if="{{item.decorateType}}"><text class="item-layout-text">{{item.decorateType}}</text></block><block wx:if="{{item.state==8&&item.style}}"><text class="item-layout-text">{{item.style}}</text></block></view></view>