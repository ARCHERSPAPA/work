}
.select-all-option .select-each-all {
    height: 831.52rpx;
    width: 100%;
    margin-bottom: 148.55rpx;
}
.select-all-option .select-all-click {
    width: 100%;
    height: 137.68rpx;
    background: white;
    box-shadow: 0px -1px 0px rgba(0, 0, 0, 0.1);
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
            justify-content: space-around;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    position: absolute;
    bottom: 0;
    z-index: 999;
}
.select-all-option .select-all-click view {
      width: 217.39rpx;
      height: 79.71rpx;
      border: 1px solid #ff8800;
      opacity: 1;
      border-radius: 22px;
      font-size: 28.98rpx;
      font-family: 'PingFang SC';
      font-weight: 400;
      color: #ff8800;
      text-align: center;
      line-height: 79.71rpx;
}
.select-all-option .select-all-click view:last-child {
        color: white;
        background: #ff8800;
}
