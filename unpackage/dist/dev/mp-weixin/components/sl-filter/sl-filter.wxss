/* 自定义-文字颜色 */
@font-face {
	font-family: 'sl-font';
	src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
}
.sl-font {
	font-family: "sl-font" !important;
	font-size: 16px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.sl-down:before {
	content: "\E61A";
}
.sl-up:before {
	content: "\E61C";
}
.font-color-activity {
  color: #FA322D;
}
.uni-spacing-col-lg {
  font-size: 24rpx;
}
