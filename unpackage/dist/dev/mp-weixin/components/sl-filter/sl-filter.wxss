              flex-wrap: wrap;
}
.select-all-option-each-con {
      height: 57.97rpx;
      border-radius: 36.23rpx;
      font-size: 25.36rpx;
      text-align: center;
      box-sizing: border-box;
      margin-left: 14.49rpx;
      margin-bottom: 14.49rpx;
