        font-size: 28.98rpx;
        display: block;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
}
.select-all-option {
  width: 100%;
  height: 940.21rpx;
  margin-top: 16.3rpx;
}
.select-all-option-each {
    font-weight: 400;
    padding-left: 14.49rpx;
    padding-right: 28.98rpx;
