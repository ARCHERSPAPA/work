/* 自定义-列表文字大小 */
.price-result {
  font-family: 'PingFang SC';
  font-weight: 400;
  background: #ff8800;
  height: 100vh;
}
.price-result-term {
    width: 100%;
    height: 184.78rpx;
    background: #ff8800;
    font-size: 28.98rpx;
    padding-top: 43.47rpx;
    box-sizing: border-box;
}
.price-result-term-title {
      color: rgba(255, 255, 255, 0.8);
      margin-left: 28.98rpx;
}
.price-result-middle {
    height: 155.79rpx;
    background: rgba(0, 0, 0, 0.03);
    color: rgba(255, 255, 255, 0.8);
    padding-top: 14.49rpx;
    box-sizing: border-box;
}
.price-result-middle-eva {
      margin-left: 28.98rpx;
      font-size: 28.98rpx;
      margin-bottom: 14.49rpx;
}
.price-result-middle-mind {
      margin: 0 28.98rpx;
      font-size: 25.36rpx;
}
.price-result-all {
    padding-bottom: 28.98rpx;
    box-sizing: border-box;
    background: #ff8800;
}
.price-result-list {
    margin-top: 28.98rpx;
    padding: 0 28.98rpx;
    box-sizing: border-box;
}
.price-result-list-desc {
      height: 72.46rpx;
      line-height: 72.46rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      margin-bottom: 14.49rpx;
      color: #FFFFFF;
}
.price-result-list-desc-left {
        font-size: 32.6rpx;
        font-weight: bold;
}
.price-result-list-desc-right {
        color: rgba(255, 255, 255, 0.8);
        font-size: 25.36rpx;
}
.price-result-list-con {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      background: white;
      box-shadow: 0px 14.49rpx 28.98rpx rgba(0, 0, 0, 0.05);
      opacity: 1;
      border-radius: 14.49rpx;
      height: 246.37rpx;
}
.price-result-list-con-img {
        width: 173.91rpx;
        height: 246.37rpx;
        border-radius: 14.49rpx 0px 0px 14.49rpx;
}
.price-result-list-con-img image {
          width: 100%;
          height: 100%;
          border-radius: 14.49rpx 0px 0px 14.49rpx;
}
.price-result-list-con-middle {
        font-size: 25.36rpx;
        color: rgba(0, 0, 0, 0.4);
        padding-top: 43.47rpx;
        box-sizing: border-box;
}
.price-result-list-con-middle view {
          margin-bottom: 14.49rpx;
}
.price-result-list-con-middle view:nth-of-type(1) {
            font-size: 43.47rpx;
            font-weight: bold;
            color: rgba(0, 0, 0, 0.9);
}
.price-result-list-con-money {
        font-size: 43.47rpx;
        font-weight: bold;
        color: #ff8800;
        padding-top: 43.47rpx;
        box-sizing: border-box;
        margin-right: 28.98rpx;
}

