.poster-canvas {
  position: fixed;
  top: -10000px;
  left: 0;
}
.poster {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999999;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.4);
  overflow: hidden;
}
.poster-close {
  display: inline-block;
  position: fixed;
  right: 2%;
  top: 1%;
  z-index: 100;
}
.poster-img {
  width: 90%;
  height: 85%;
  position: fixed;
  left: 5%;
  right: 5%;
  top: 5%;
  bottom: 10%;
  z-index: 99;
  overflow: hidden;
}
.poster-img .image {
  width: 100%;
  height: 100%;
}
.poster-btns {
  width: 100%;
  height: 20%;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 99;
  text-align: center;
}
.poster-btns-item {
  display: inline-block;
  background: #098684;
  font-size: 28.98rpx;
  font-weight: bold;
  width: 246.37rpx;
  height: 79.71rpx;
  line-height: 79.71rpx;
  color: white;
  border-radius: 39.85rpx;
  position: absolute;
  left: 50%;
  margin-left: -123.18rpx;
  bottom: 10%;
}
.example {
  position: relative;
}
.example-wrap {
  position: relative;
}
.example-operate {
  position: fixed;
  width: 100%;
  bottom: 0;
  height: 108.69rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  background-color: #fff;
}
.example-operate-flex1 {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}
.example-operate-flex1 .operate-btn {
  display: block;
  outline: none;
  line-height: 1.5;
  background: transparent;
}
.example-operate-flex1 .operate-btn::after {
  border: 0;
}
.example-operate-flex1 .operate-text {
  display: block;
  text-align: center;
  font-size: 21.73rpx;
  font-weight: 400;
  line-height: 30.79rpx;
  color: rgba(0, 0, 0, 0.7);
  margin-top: -7.62rpx;
}
.example-operate-flex1 .operate-text.actived {
  color: #098684;
}
.example-operate-flex2 {
  -webkit-box-flex: 2.5;
  -webkit-flex: 2.5;
          flex: 2.5;
}
.example-operate-flex2 .operate-btn {
  width: 90%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  background: #098684;
  color: white;
  font-size: 28.98rpx;
  font-weight: bold;
  border-radius: 39.85rpx;
}
.example-operate-flex2 .operate-btn::after {
  border: 0;
}
