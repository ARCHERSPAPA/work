<view data-event-opts="{{[['tap',[['goContract',['$0'],['order']]]]]}}" class="order-info" catchtap="__e"><view class="order-base"><view class="order-base-company"><view class="order-base-company-info"><text class="company-mark">企</text><text class="company-name">{{order.companyName?order.companyName:''}}</text><text class="company-person">{{"设计师："+(order.applyName?order.applyName:'')}}</text></view><view class="order-base-company-status"><text>{{$root.m0}}</text></view></view><view class="order-base-id"><text>{{"订单ID："+(order.quoteNo?order.quoteNo:'')}}</text></view></view><block wx:if="{{order&&(order.customerAddress||order.price||customerHouseArea)}}"><view class="order-custom"><view class="order-custom-base"><text class="custom-base">{{order.customerAddress?order.customerAddress:''}}</text><block wx:if="{{order&&order.price}}"><text class="custom-price">{{"¥"+$root.f0}}</text></block></view><block wx:if="{{order&&order.customerHouseArea}}"><view class="order-custom-area"><text>{{"房屋面积："+order.customerHouseArea+"平米"}}</text></view></block></view></block><view class="order-operate"><text class="order-operate-time">{{$root.f1}}</text><view class="order-operate-btn"><button>合同</button></view></view></view>