<block wx:if="{{list.length>0}}"><view class="mkb-price-area"><view class="mkb-price-area-top"><view class="mkb-price-area-title">您的房屋面积</view><input class="up-number" type="number" disabled="{{flag}}" placeholder="请填写房屋面积" placeholder-style="font-size:43.47rpx;font-weight:bold;color:rgba(0,0,0,0.40);" focus="{{getFocus}}" data-event-opts="{{[['blur',[['onBlur',['$event']]]],['focus',[['onFocus',['$event']]]],['input',[['__set_model',['','priceArea','$event',[]]],['onInput',['$event']]]]]}}" value="{{priceArea}}" bindblur="__e" bindfocus="__e" bindinput="__e"/><view class="mkb-price-area-title">户型</view><block wx:if="{{currentHouseType[index]}}"><view class="uni-list"><view class="uni-list-cell"><view class="uni-list-cell-db"><picker value="{{index}}" range="{{currentHouseType}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{currentHouseType[index]}}</view></picker></view></view></view></block><block wx:else><view class="uni-list">暂无</view></block></view><view class="mkb-price-area-title type">房屋装修类型</view><view class="mkb-price-area-select"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',[index,'$0'],[[['list','',index,'decorateStyle']]]]]]]}}" class="{{['mkb-price-area-select-outer',index==idx?'mkb-price-area-select-outer active':'mkb-price-area-select-outer']}}" bindtap="__e"><view class="mkb-price-area-select-img"><image src="{{item.iconImg}}"></image></view><view class="{{[index==idx?'mkb-style-active':'mkb-price-area-select-item']}}">{{''+item.decorateStyle+''}}</view></view></block></view><view class="mkb-price-area-btn"><button disabled="{{isDisabled}}" data-event-opts="{{[['tap',[['next']]]]}}" catchtap="__e">{{title}}</button></view></view></block>